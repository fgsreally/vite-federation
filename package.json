{
  "name": "vite-federation",
  "version": "0.0.5",
  "license": "MIT",
  "author": "fgs",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fgsreally/vite-federation"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/",
      "require": "./dist/"
    },
    "./helper": {
      "import": "./dist/helper/index.js",
      "require": "./dist/helper/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "dist/index.d.ts"
      ],
      "helper": [
        "dist/helper/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "watch": "tsup --watch",
    "preview": "vite preview",
    "example:remote": "npm -C examples/remote run build:remote",
    "example:home": "npm -C examples/home run dev",
    "e2e": "cypress open",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vue/devtools-api": "^6.2.1",
    "axios": "^0.27.2",
    "colors": "^1.4.0",
    "content-hash": "^2.5.2",
    "es-module-lexer": "^0.10.5",
    "fs-extra": "^10.1.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^3.0.0",
    "@cypress/vue": "^3.1.0",
    "@rollup/pluginutils": "^4.2.1",
    "@types/download": "^8.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.40",
    "@vitejs/plugin-vue": "^2.3.3",
    "commitizen": "^4.2.5",
    "cypress": "^10.2.0",
    "magic-string": "^0.26.4",
    "rollup": "^2.75.5",
    "tsup": "^6.1.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  },
  "engines": {
    "node": ">=14"
  }
}
