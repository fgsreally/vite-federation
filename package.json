{
  "name": "vite-federation",
  "version": "1.0.0",
  "license": "MIT",
  "author": "fgs",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fgsreally/vite-federation"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/",
      "require": "./dist/"
    },
    "./helper": {
      "import": "./dist/helper/index.js",
      "require": "./dist/helper/index.cjs",
      "types": "./dist/helper/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "dist/index.d.ts"
      ],
      "helper": [
        "dist/helper/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsup && npm run build:view",
    "build:view": "vite build",
    "watch": "tsup --watch",
    "example:remote": "npm -C examples/remote run build:remote --watch",
    "example:home": "npm -C examples/home run dev:static",
    "e2e": "cypress open"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vue/devtools-api": "^6.2.1",
    "axios": "^0.27.2",
    "colors": "^1.4.0",
    "content-hash": "^2.5.2",
    "es-module-lexer": "^0.10.5",
    "fs-extra": "^10.1.0",
    "magic-string": "^0.26.4",
    "semver": "^7.3.8",
    "sirv": "^2.0.2",
    "vite": "^3.1.0"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^3.0.0",
    "@cypress/vue": "^3.1.0",
    "@rollup/pluginutils": "^4.2.1",
    "@types/download": "^8.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.40",
    "@types/semver": "^7.3.12",
    "cypress": "^10.2.0",
    "graphology": "^0.25.1",
    "rollup": "^2.75.5",
    "sigma": "^2.4.0",
    "tsup": "^6.1.2",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.22.9",
    "vue-tsc": "^0.34.7"
  },
  "engines": {
    "node": ">=14"
  }
}